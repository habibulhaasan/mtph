        ];

        // -------------------------------------------------------
        // DROPDOWN LOGIC
        // -------------------------------------------------------
        const divisionSelect = document.getElementById('division');
        const districtSelect = document.getElementById('district');
        const upazilaSelect = document.getElementById('upazila');

        // Load Divisions on Start
        divisions.forEach(div => {
            const option = document.createElement('option');
            option.value = div.id; 
            option.textContent = div.bn_name;
            divisionSelect.appendChild(option);
        });

        // Handle Division Change
        divisionSelect.addEventListener('change', function() {
            const divId = this.value;
            
            // Reset Child Dropdowns
            districtSelect.innerHTML = '<option value="" disabled selected>জেলা নির্বাচন করুন</option>';
            upazilaSelect.innerHTML = '<option value="" disabled selected>আগে জেলা দিন</option>';
            districtSelect.disabled = false;
            upazilaSelect.disabled = true;
            districtSelect.classList.remove('bg-slate-100');
            upazilaSelect.classList.add('bg-slate-100');

            // Filter Districts
            const filteredDistricts = districts.filter(d => d.division_id === divId);
            
            filteredDistricts.forEach(dist => {
                const option = document.createElement('option');
                option.value = dist.id; 
                option.textContent = dist.bn_name;
                districtSelect.appendChild(option);
            });
        });

        // Handle District Change
        districtSelect.addEventListener('change', function() {
            const distId = this.value;
            
            // Reset Upazila Dropdown
            upazilaSelect.innerHTML = '<option value="" disabled selected>উপজেলা নির্বাচন করুন</option>';
            upazilaSelect.disabled = false;
            upazilaSelect.classList.remove('bg-slate-100');

            // Filter Upazilas
            const filteredUpazilas = upazilas.filter(u => u.district_id === distId);
            
            filteredUpazilas.forEach(upa => {
                const option = document.createElement('option');
                option.value = upa.bn_name; 
                option.textContent = upa.bn_name;
                upazilaSelect.appendChild(option);
            });
        });


        // -------------------------------------------------------
        // FORM SUBMISSION LOGIC
        // -------------------------------------------------------
        const form = document.getElementById('dataForm');
        const loadingOverlay = document.getElementById('loadingOverlay');
        const successModal = document.getElementById('successModal');

        const SCRIPT_URL = 'https://script.google.com/macros/s/AKfycbyC7JA3qaZca2jMyxuIGWs22OtOE8qXIv4DUU0ggZpgN6Jg71exT5XxoQnabwCR6HxApw/exec'; 

        form.addEventListener('submit', function(e) {
            e.preventDefault();
            
            const formData = new FormData(form);
            
            const divName = divisionSelect.options[divisionSelect.selectedIndex].text;
            const distName = districtSelect.options[districtSelect.selectedIndex].text;
            
            formData.set('division', divName);
            formData.set('district', distName);
            
            loadingOverlay.classList.remove('hidden');

            const data = new URLSearchParams();
            for (const pair of formData) {
                data.append(pair[0], pair[1]);
            }

            fetch(SCRIPT_URL, {
                method: 'POST',
                body: data
            })
            .then(response => response.json())
            .then(result => {
                loadingOverlay.classList.add('hidden');
                if(result.result === 'success') {
                    successModal.classList.remove('hidden');
                    form.reset();
                } else {
                    alert("Error: " + result.error);
                }
            })
            .catch(error => {
                console.log('Opaque response or error', error);
                // GAS sometimes returns opaque response for CORS, treating as success if no network error
                loadingOverlay.classList.add('hidden');
                successModal.classList.remove('hidden');
                form.reset();
            });
        });

    </script>
</body>
</html>